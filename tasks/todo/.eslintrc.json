{
  "extends": [
      "airbnb",
      "plugin:react/recommended"
  ],
  "parser": "babel-eslint",
  "env": {
      "browser": true,
      "node": true,
      "es6": true
  },
  "rules": {
    "object-curly-spacing": ["error", "never"],
    "no-unused-expressions": ["error", {"allowShortCircuit": true}],
    "react/jsx-filename-extension": 0,
    "comma-dangle": 0,
    "import/no-extraneous-dependencies": 0
  }
}
